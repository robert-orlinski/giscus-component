{
  "name": "@giscus/react",
  "private": false,
  "description": "Type-safe React component for giscus",
  "version": "1.0.1",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "lint": "tsc",
    "build": "run-p build:*",
    "build:script": "vite build",
    "build:types": "dts-bundle-generator --no-banner --external-imports=react -o dist/index.d.ts lib/index.ts",
    "serve": "vite preview"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/node": "^16.0.0",
    "@types/vfile-message": "^2.0.0",
    "commitizen": "^4.2.4",
    "commitlint-config-gitmoji": "^2.2.5",
    "cz-emoji": "^1.3.1",
    "dts-bundle-generator": "^5.9.0",
    "husky": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "semantic-release": "^17.4.4",
    "semantic-release-gitmoji": "^1.3.4",
    "semantic-release-monorepo": "^7.0.5",
    "typescript": "^4.3.5",
    "vite": "^2.4.1",
    "wsrun": "^5.2.4"
  },
  "author": {
    "name": "laymonage",
    "email": "me@laymonage.com",
    "url": "https://laymonage.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/giscus/giscus-component.git",
    "directory": "packages/react"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "giscus",
    "comment",
    "comments",
    "comment-system",
    "react",
    "github",
    "component",
    "discussions"
  ],
  "homepage": "https://github.com/giscus/giscus-component#readme",
  "bugs": "https://github.com/giscus/giscus-component/issues",
  "dependencies": {
    "iframe-resizer-react": "^1.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
